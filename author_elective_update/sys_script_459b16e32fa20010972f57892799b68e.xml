        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

var gdt = new GlideDateTime();
	//Adding 2yrs to the date asset was created
	gdt.addYearsLocalTime(2);
	//If warranty matches the year then add the number of years to date created
	if (current.warranty.getDisplayValue() === "2 years");{
		current.setValue("warranty_expiration_date", gdt.getValue());
	}
	

	


}) (current, previous);]]></script>
